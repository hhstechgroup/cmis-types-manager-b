<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
              http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SubTableRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSubTableRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.MenuRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSubmenuRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.AccordionPanelRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapAccordionRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.PanelRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapPanelRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.RowTogglerRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapRowTogglerRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.TreeTableRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapTreeTableRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SelectCheckboxMenuRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSelectCheckBoxMenuRenderer</renderer-class>
        </renderer>

        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SelectOneMenuRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSelectOneMenuRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SelectOneButtonRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSelectOneButtonRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.FileUploadRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapFileUploadRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SelectOneRadioRenderer</renderer-type>
            <renderer-class>com.engagepoint.renderer.primefaces.BootStrapSelectOneRadioRenderer</renderer-class>
        </renderer>
    </render-kit>


</faces-config>